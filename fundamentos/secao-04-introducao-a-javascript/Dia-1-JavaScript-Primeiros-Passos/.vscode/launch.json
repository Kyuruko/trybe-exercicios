{
    "configurations": [
    {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:5500",
        "webRoot": "${workspaceFolder}"
    },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/script.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },

        {
            "type": "node-terminal",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}/scrip.js"
        }
    ]
}